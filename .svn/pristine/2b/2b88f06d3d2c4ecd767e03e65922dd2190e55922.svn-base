/*excel table for the patient information on left drawer*/


table.tableizer-table {
    border: 1px solid #CCC; font-family: Arial, Helvetica, sans-serif
        font-size: 12px;
    margin: auto;
    margin-top: 50px;
} 
.tableizer-table td {
    padding: 4px;
    margin: 3px;
    border: 1px solid #ccc;
}
.tableizer-table th {
    background-color: #9EB252; 
    color: #FFF;
    font-weight: bold;
}